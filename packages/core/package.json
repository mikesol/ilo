{
  "name": "@mvfm/core",
  "version": "0.0.1",
  "description": "Ilo core package",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    },
    "./interpreters/core": {
      "types": "./dist/interpreters/core.d.ts",
      "default": "./dist/interpreters/core.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./trait-utils": {
      "types": "./dist/trait-utils.d.ts",
      "default": "./dist/trait-utils.js"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "default": "./dist/plugins/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check": "biome check src && tsc -p tsconfig.json && api-extractor run --local --config api-extractor.json",
    "test": "vitest run --root ../.. tests/core.test.ts tests/composition.test.ts tests/interpreters"
  },
  "dependencies": {}
}
