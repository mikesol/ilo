{
  "name": "@mvfm/plugin-pino",
  "version": "0.0.1",
  "description": "Ilo pino plugin",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./interpreter": {
      "types": "./dist/interpreter.d.ts",
      "default": "./dist/interpreter.js"
    },
    "./handler.client": {
      "types": "./dist/handler.client.d.ts",
      "default": "./dist/handler.client.js"
    },
    "./handler.server": {
      "types": "./dist/handler.server.d.ts",
      "default": "./dist/handler.server.js"
    },
    "./client-pino": {
      "types": "./dist/client-pino.d.ts",
      "default": "./dist/client-pino.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check": "biome check src && tsc -p tsconfig.json && api-extractor run --local --config api-extractor.json",
    "test": "vitest run --root ../.. tests/plugins/pino"
  },
  "dependencies": {
    "@mvfm/core": "workspace:*",
    "pino": "^10.3.1"
  }
}
